{
  "name": "wannabe-pro/openid-connect",
  "type": "library",
  "version": "0.0.2",
  "description": "Bare-bones OpenID Connect client",
  "keywords": ["openid"],
  "license": "MIT",
  "require": {
    "php": ">=5.5",
    "phpseclib/phpseclib": "~2.0",
    "ext-json": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "WannaBaPro\\OpenIDConnect\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "5.5"
    }
  }
}
